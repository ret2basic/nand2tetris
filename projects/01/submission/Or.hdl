 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // De Morgan's Law: a Or b = Not ((Not a) And (Not b))
    Not (in=a, out=nota);
    Not (in=b, out=notb);
    And (a=nota, b=notb, out=and);
    Not (in=and, out=out);
}
