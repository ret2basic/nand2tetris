 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=temp1);
    Not (in=b, out=temp2);
    And (a=temp1, b=temp2, out=temp3);
    Not (in=temp3, out=out);
}
